    "deploy": "gh-pages -d dist",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --open --config webpack.config.js",
    "lint": "eslint src/js",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run dev"
